    <sql id="selectFzuSpecialdormApprovalVo">
        select approval_id, student_id, student_name, approval_category, approval_reason, start_time, end_time, fdy_id, fdy_opinion, xgc_id, xgc_opinion, manage_id, manage_opinion, approval_status, dorm_id from fzu_specialdorm_approval
