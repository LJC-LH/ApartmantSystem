<mapper namespace="com.ruoyi.apartment.mapper.FzuSpecialdormApprovalMapper">

    <resultMap type="FzuSpecialdormApproval" id="FzuSpecialdormApprovalResult">
        <result property="approvalId"    column="approval_id"    />
